    <TextView
        android:id="@+id/cardLastNameTo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/transfer_cover7"
        android:layout_alignTop="@+id/transfer_cover7"
        android:layout_marginStart="17dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/dm_sans_medium"
        android:text="@string/recipient_card_lastname"
        android:textColor="@color/black"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/cardPatronymicTo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/transfer_cover8"
        android:layout_alignTop="@+id/transfer_cover8"
        android:layout_marginStart="17dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/dm_sans_medium"
        android:text="@string/recipient_patronymic"
        android:textColor="@color/black"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/cardNameTo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/transfer_cover6"
        android:layout_alignTop="@+id/transfer_cover6"
        android:layout_marginStart="17dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/dm_sans_medium"
        android:text="@string/recipient_card_name"
        android:textColor="@color/black"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/moneyTransfer3"
        android:layout_width="241dp"
        android:layout_height="84dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="19dp"
        android:fontFamily="@font/roboto_bold"
        android:gravity="top"
        android:lineSpacingExtra="7sp"
        android:text="@string/transfer_money2"
        android:textAppearance="@style/lunapay_main"
        android:translationY="-7.52sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/transfer_cover5"
        android:layout_width="300dp"
        android:layout_height="65dp"
        android:layout_below="@+id/ellipsTo"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/currency_cover"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/moneyTransfer3" />

    <EditText
        android:id="@+id/editTextNumberDecimal4"
        android:layout_width="274dp"
