    <EditText
        android:id="@+id/editTextNumberDecimal6"
        android:layout_width="276dp"
        android:layout_height="25dp"
        android:layout_below="@+id/cardLastNameTo"
        android:layout_alignStart="@+id/transfer_cover7"
        android:layout_alignBottom="@+id/transfer_cover7"
        android:layout_marginStart="9dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="5dp"
        android:ems="10"
        android:editable="false"
        android:inputType="numberDecimal"
        android:textColor="#0B0A0A"
        android:textSize="12sp"
        tools:ignore="Deprecated" />

    <TextView
        android:id="@+id/cardTo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/transfer_cover5"
        android:layout_alignTop="@+id/transfer_cover5"
        android:layout_marginStart="17dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/dm_sans_medium"
        android:text="@string/recipient_card"
        android:textColor="@color/black"
        android:textSize="10sp" />

    <ImageView
        android:id="@+id/ellipsTo"
        android:layout_width="97dp"
        android:layout_height="68dp"
        android:layout_below="@+id/moneyTransfer3"
        android:layout_alignParentStart="true"
        android:layout_marginStart="239dp"
        android:layout_marginTop="-37dp"
        app:srcCompat="@drawable/ellipse_44" />

    <ImageView
        android:id="@+id/ellips"
        android:layout_width="32dp"
        android:layout_height="33dp"
        android:layout_alignEnd="@+id/ellipsTo"
        android:layout_alignBottom="@+id/ellipsTo"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="19dp"
        app:srcCompat="@drawable/path_395" />

</RelativeLayout>