        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/currency_cover"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/transfer_cover7"
        android:layout_width="300dp"
        android:layout_height="65dp"
        android:layout_below="@+id/transfer_cover6"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/currency_cover"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/transfer_cover8"
        android:layout_width="300dp"
        android:layout_height="65dp"
        android:layout_below="@+id/transfer_cover7"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/currency_cover"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/transfer_cover9"
        android:layout_width="300dp"
